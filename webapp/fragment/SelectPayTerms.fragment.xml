<core:FragmentDefinition xmlns="sap.m" 
    xmlns:core="sap.ui.core">
    <TableSelectDialog noDataText="{i18n>NenhumaCondicaoEncontrada}" title="{i18n>SelecionaACondicao}" search="handleFornecedorSearch" confirm="confirmAddFornecedoresDoItem" items="{ path : 'jModel>/PaymentTerms', sorter : { path : 'Name', descending : false } }" rememberSelections="false">
        <ColumnListItem>
            <cells>
                <ObjectIdentifier title="{Title}" text="{Title}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column width="12em">
                <header>
                    <Text text="{i18n>Condicao}"/>
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>
