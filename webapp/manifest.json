{
  "_version": "1.12.0",
  "sap.app": {
    "id": "br.com.patrimar.criacotacao",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    },
    "dataSources": {
      "mainService": {
        "uri": "/S4_HANA/sap/opu/odata/SAP/ZPORTAL_COTACAO_SRV/",
        "type": "OData",
        "settings": {
          "localUri": "localService/ZSD_ODATA_PRICE_COCKPIT_SRV/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "br.com.patrimar.criacotacao.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.72.0",
      "libs": {
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "br.com.patrimar.criacotacao.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Request",
          "useBatch": false
        },
        "dataSource": "mainService",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "br.com.patrimar.criacotacao.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "List",
          "pattern": "",
          "target": [
            "List"
          ]
        },
        {
          "name": "Detail",
          "pattern": "Detail/{key}",
          "target": [
            "Detail"
          ]
        }
      ],
      "targets": {
        "List": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "List",
          "viewName": "List",
          "viewLevel": 1
        },
        "Detail": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Detail",
          "viewName": "Detail",
          "viewLevel": 2
        }
      }
    }
  }
}